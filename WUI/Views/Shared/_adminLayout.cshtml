<!DOCTYPE html>
<html lang="fr">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <title>WebSport | @ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
          rel="stylesheet">
    @Styles.Render("~/Content/Admin.css")
    @Styles.Render("~/Content/Bootstrap/bootstrap.min.css")
    @RenderSection("AdditionalMeta", false)

    <link href="https://cdnjs.cloudflare.com/ajax/libs/select2-bootstrap-theme/0.1.0-beta.6/select2-bootstrap.css" rel="stylesheet" />

</head>
<body>
    <div class="container container-table">
        <div class="row">
            <div class="col-lg-2 col-md-2 col-sm-2 col-xs-1 navbar-right">
                @Html.Partial("_adminMenu")
            </div>
            <div class="col-lg-10 col-md-10 col-xs-10 col-sm-10 offset-lg-2 offset-md-2 offset-xs-2 offset-sm-2">
                @RenderBody()
            </div>
        </div>
    </div>


    @Scripts.Render("~/Scripts/jquery-1.10.2.js")
    @Scripts.Render("~/Scripts/Bootstrap/bootstrap.js")
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    @RenderSection("scripts", false)

</body>
</html>